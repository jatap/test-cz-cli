{
  "name": "test-cz-cli",
  "version": "1.2.1",
  "description": "Test Commitizen",
  "main": "src/index.js",
  "scripts": {
    "cm": "git cz",
    "postcommit": "npm install && git add package-lock.json && git commit -m 'chore: Update package-lock.json'",
    "release": "standard-version -t ''",
    "push": "git push --follow-tags origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jatap/test-cz-cli.git"
  },
  "keywords": [
    "Commitizen"
  ],
  "author": "Julio Antúnez Tarín <julio.antunez.tarin@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jatap/test-cz-cli/issues"
  },
  "homepage": "https://github.com/jatap/test-cz-cli#readme",
  "devDependencies": {
    "commitizen": "^2.9.6",
    "husky": "^0.14.3",
    "standard-version": "^4.3.0"
  }
}
